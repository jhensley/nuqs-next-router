{
  "$schema": "https://turbo.build/schema.v2.json",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "nuqs#build": {
      "outputs": ["dist/**", "size.json"]
    },
    "nuqs#test": {
      "outputs": ["dist/**", "coverage/**"],
      "dependsOn": ["build"]
    }
  }
}
